{
  "compilerOptions": {
    "lib": [
      "ES2021",
      "WebWorker",
      "DOM"
    ],
    "target": "ES2022",
    "moduleResolution": "Node",
    "outDir": ".",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": [
      "@cloudflare/workers-types"
    ],
  },
  "include": [
    "index.ts"
  ],
  "exclude": []
}